{
  "name": "blog-admin-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "firebase": "^12.6.0",
    "marked": "^5.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.0.5",
    "@testing-library/react": "^14.0.0",
    "@types/marked": "^5.0.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.14",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.1.7",
    "vitest": "^1.4.8"
  },
  "vitest": {
    "setupFiles": [
      "src/setupTests.ts"
    ],
    "environment": "jsdom",
    "include": [
      "src/**/*.{test,spec}.{js,mjs,cjs,ts,tsx}"
    ],
    "exclude": [
      "**/e2e/**",
      "**/tests/**"
    ]
  }
}
